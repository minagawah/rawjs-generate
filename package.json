{
  "name": "rawjs-generate",
  "version": "0.1.0",
  "private": false,
  "description": "A sample RawJS app with PUB/SUB messaging to generate/destroy DOM elements",
  "main": "dist/index.html",
  "types": "types/index.d.ts",
  "scripts": {
    "eslint": "eslint --ext js \"src/**/*.js\"",
    "eslint:fix": "eslint --fix --ext js \"src/**/*.js\"",
    "prettier": "prettier --write \"*.{js,md,json}\" \"src/**/*.js\"",
    "lint": "$npm_execpath run prettier && $npm_execpath run eslint:fix",
    "type:check": "tsc -p tsconfig.json",
    "type:generate": "rimraf types && tsc -p tsconfig.generate.json",
    "jsdoc": "rimraf jsdoc && jsdoc -c jsdoc.conf.js -d jsdoc/",
    "test": "jest",
    "build:css": "postcss src/styles --dir dist",
    "watch:css": "nodemon -x $npm_execpath run build:css -e css -w src/styles",
    "watch:js": "webpack serve --config webpack.dev.js",
    "build:js": "webpack --config webpack.prod.js",
    "clean": "rimraf dist",
    "build": "$npm_execpath run clean && $npm_execpath run build:css && $npm_execpath run build:js",
    "start": "concurrently \"$npm_execpath run watch:css\" \"$npm_execpath run watch:js\""
  },
  "repository": "https://github.com/minagawah/rawjs-generate.git",
  "keywords": [
    "RawJS"
  ],
  "author": "minagawah <minag@tokyo800.jp>",
  "license": "(Unlicense OR MIT)",
  "bugs": {
    "url": "https://github.com/minagawah/rawjs-generate/issues"
  },
  "dependencies": {
    "@squaresapp/rawjs": "^1.3.12",
    "core-js": "^3.35.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@stylistic/eslint-plugin": "^1.5.4",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-preval": "^5.1.0",
    "concurrently": "^8.2.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "jsdoc-plugin-intersection": "^1.0.4",
    "jsdoc-tsimport-plugin": "^1.0.5",
    "license-webpack-plugin": "^4.0.2",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.2",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.0",
    "postcss-loader": "^7.3.4",
    "postcss-mixins": "^9.0.4",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.3.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
